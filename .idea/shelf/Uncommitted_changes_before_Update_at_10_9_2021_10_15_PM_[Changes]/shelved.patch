\r\n\r\n        var colors = new Map();\r\n        colors.set('r', 0);\r\n        colors.set('g', 0);\r\n        colors.set('b', 0);\r\n\r\n        function bin2int(binary){\r\n            {#console.log(binary)#}\r\n            return parseInt(binary, 2) //turns string into number, comma 2 specifies that input number is written in base 2\r\n        }\r\n\r\n\r\n\r\n        function totalRow (color) {    // function that calculates the decimal value for a certain row after one of its buttons is toggled\r\n            let binary = \"\";\r\n            for (var i = 1; i <= 8; i++) {\r\n                let element = document.getElementById(color + i);\r\n                binary += parseInt(element.innerHTML);\r\n            }\r\n            sum = bin2int(binary)\r\n\r\n            document.getElementById(color).innerHTML = \"Decimal value: \" + sum;\r\n            colors.set(color, sum)\r\n            setColorCode();\r\n        }\r\n\r\n\r\n        function setColorCode() {    // uses values from each row to create a color\r\n            let r = colors.get(\"r\").toString(16);\r\n            let g = colors.get(\"g\").toString(16);\r\n            let b = colors.get(\"b\").toString(16);\r\n            if (r.length == 1) {\r\n                r = \"0\" + r;\r\n            }\r\n            if (g.length == 1) {\r\n                g = \"0\" + g;\r\n            }\r\n            if (b.length == 1) {\r\n                b = \"0\" + b;\r\n            }\r\n            let code = \"#\" + r + g + b;\r\n            document.getElementById(\"color\").style.backgroundColor = code;\r\n            document.getElementById(\"code\").innerHTML = \"Hexcode = \" + code;\r\n\r\n        }\r\n\r\n        function toggleSwitch (id) {     // toggles button between on and off\r\n            const element = document.getElementById(id);\r\n            if (element.innerHTML == \"1\") {\r\n                element.innerHTML = \"0\";\r\n            } else {\r\n                element.innerHTML = \"1\";\r\n            }\r\n            element.classList.toggle(\"On\");\r\n            element.classList.toggle(\"Off\");\r\n\r\n            totalRow(id.charAt(0));\r\n        }\r\n\r\n        {#function reset () {   // function to reset all buttons back to off#}\r\n        {#    colors = {\"r\", \"g\", \"b\"}#}\r\n        {#    for (var j = 0; j < 3; j++) {#}\r\n        {#        for (var i = 1; i <= 8; i++) {#}\r\n        {#            console.log(colors[j] + i)#}\r\n        {#            document.getElementById(colors[j] + i).innerHTML = \"0\";#}\r\n        {#            totalRow(colors[j]);#}\r\n        {#            document.getElementById(colors[j] + i).classList.add(\"Off\");#}\r\n        {#            document.getElementById(colors[j] + i).classList.remove(\"On\");#}\r\n        {#        }#}\r\n        {#    }#}\r\n        {#}#}\r\n\r\n            function reset () {   // function to reset all buttons back to off\r\n                for (var i = 1; i <= 8; i++) {\r\n                    document.getElementById('r' + i).innerHTML = \"0\";\r\n                    document.getElementById('g' + i).innerHTML = \"0\";\r\n                    document.getElementById('b' + i).innerHTML = \"0\";\r\n                    calculateRow('r');\r\n                    calculateRow('b');\r\n                    calculateRow('g');\r\n                    document.getElementById('r' + i).classList.add(\"buttonOff\");\r\n                    document.getElementById('g' + i).classList.add(\"buttonOff\");\r\n                    document.getElementById('b' + i).classList.add(\"buttonOff\");\r\n                    document.getElementById('r' + i).classList.remove(\"buttonOn\");\r\n                    document.getElementById('g' + i).classList.remove(\"buttonOn\");\r\n                    document.getElementById('b' + i).classList.remove(\"buttonOn\");\r\n                }\r\n            }\r\n        window.onload = reset();  // init buttons\r\n\r\n    </script>\r\n\r\n{% endblock %}\r\n\r\n{% block background %}\r\n    <body style=\"background-color:darkgoldenrod;\">  </body>\r\n{% endblock %}\r\n
